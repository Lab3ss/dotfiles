URxvt.intensityStyles:	false

URxvt.background: [70]#1d1d1d

URxvt.colorUL:		#4682B4

URxvt.font: 		xft:DejaVu Sans Mono:size=9

URxvt.internalBorder: 10
URxvt.depth:		32

URxvt.scrollBar:	false

URxvt.cursorColor:	#00AA88
URxvt.cursorColor2:	#00AA88
URxvt.cursorBlink: 1

URxvt.tripleclickwords: 1

!! Extensions
URxvt.perl-ext-common:default,matcher,resize-font,url-select,keyboard-select,clipboard
URxvt.perl-lib: /home/lab3ss/Development/dotfiles/urxvt/perl-ext

URxv.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.underline: true
URxvt.url-select.launcher: google-chrome

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! resize-font
URxvt.keysym.M-j: resize-font:smaller
URxvt.keysym.M-k: resize-font:bigger

!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1


rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.lines:		10
rofi.bw:		0
rofi.opacity:		"10"
rofi.hide-scrollbar:	true
rofi.width:		30
